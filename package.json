    "dev:no-proxy": "cross-env NO_PROXY=true contextPath= npm run dev",
    "mock": "cd mock&&cross-env port=3004 node server.js",
    "dev-mock": "concurrently -p \"[{name}]\" -n \"mock,roadhog\" -c \"bgMagenta.bold,bgBlue.bold\" \"npm run mock\" \"npm run dev\" \"cd mock&&cd json&& json-server db.js\" ",
    "lint": "eslint --fix --ext .js src",
    "build": "cross-env NO_PROXY=true DISABLE_ESLINT=true contextPath= roadhog build && node after.build.js",
    "test": "jest --config jest.json",
    "precommit": "lint-staged",
    "buildTheme": "node roadhogrc.build.js && node after.build.js"
  },
  "lint-staged": {
    "{src,mock,__tests__}/**/*.{js,json,css,md,less}": [
      "prettier --write",
      "git add"
    ]
